<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="5">
    <Name Value="SimpleWebServerGUI"/>
    <Type Value="RunAndDesignTime"/>
    <Author Value="Mattias GÃ¤rtner"/>
    <CompilerOptions>
      <Version Value="11"/>
      <SearchPaths>
        <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)/$(LCLWidgetType)"/>
      </SearchPaths>
    </CompilerOptions>
    <Description Value="A web server used by Pas2js (Pascal to JavaScript transpiler)."/>
    <License Value="GPL-2"/>
    <Version Major="1"/>
    <Files>
      <Item>
        <Filename Value="simplewebsrvwnd.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="SimpleWebSrvWnd"/>
      </Item>
      <Item>
        <Filename Value="simplewebsrvoptionsframe.pas"/>
        <UnitName Value="SimpleWebSrvOptionsFrame"/>
      </Item>
      <Item>
        <Filename Value="simplewebsrvoptions.pas"/>
        <UnitName Value="SimpleWebSrvOptions"/>
      </Item>
      <Item>
        <Filename Value="simplewebsrvutils.pas"/>
        <UnitName Value="SimpleWebSrvUtils"/>
      </Item>
      <Item>
        <Filename Value="simplewebsrvcontroller.pas"/>
        <UnitName Value="SimpleWebSrvController"/>
      </Item>
      <Item>
        <Filename Value="simplewebsrvstrconsts.pas"/>
        <UnitName Value="SimpleWebSrvStrConsts"/>
      </Item>
      <Item>
        <Filename Value="simplewebsrvadd.pas"/>
        <UnitName Value="SimpleWebSrvAdd"/>
      </Item>
    </Files>
    <i18n>
      <EnableI18N Value="True"/>
      <OutDir Value="languages"/>
    </i18n>
    <RequiredPkgs>
      <Item>
        <PackageName Value="CodeTools"/>
      </Item>
      <Item>
        <PackageName Value="SynEditDsgn"/>
      </Item>
      <Item>
        <PackageName Value="FCL"/>
      </Item>
    </RequiredPkgs>
    <UsageOptions>
      <UnitPath Value="$(PkgOutDir)"/>
    </UsageOptions>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
  </Package>
</CONFIG>
